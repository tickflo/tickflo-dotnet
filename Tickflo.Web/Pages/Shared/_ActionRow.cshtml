@model Tickflo.Web.Pages.Shared.ActionRowView
<div class="join md:join">
  @foreach (var action in Model.Actions)
  {
    if (!string.IsNullOrWhiteSpace(action.Href) && !action.Disabled)
    {
      <a class="@action.ButtonClass join-item btn-sm" href="@action.Href">@action.Label</a>
    }
    else
    {
      <button class="@action.ButtonClass join-item btn-sm" disabled>@action.Label</button>
    }
  }
</div>
